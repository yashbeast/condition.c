#include<stdio.h>
main()
{
	printf("pick an item : \n1 pizza\n2 burger\n3 pasta\n4 french fries\n5 sandwich");
	int choice=0;
	scanf("%d",&choice);
	switch(choice)
	{
	
	case 1:
		printf("food item-pizza\n prize-Rs239");
		break;
	case 2:
		printf("food item-burger\n prize-Rs129");
		break;
	case 3:
		printf("food item-pasta\n prize-Rs179");
		break;
	case 4:
		printf("food item-french fries\n prize-Rs99");
		break;
	case 5:
		printf("food itme-sandwich\n prize-Rs149");
		break;
	default :
		printf("invalid");
	}
}
